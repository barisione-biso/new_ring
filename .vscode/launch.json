    "version": "0.2.0",
    "configurations": [
        {
            "name": "Query index (orden promisorio)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/query-index",
            "args": ["~/dcc_uchile/git_projects/data/wikidata-filtered-enumerated.dat.ring",
                    "${workspaceFolder}/Queries/Queries-wikidata-benchmark.txt",
                    "one_ring_muthu_leap_adaptive"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Build index (orden promisorio)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/build-index",
            "args": ["~/dcc_uchile/git_projects/data/wikidata-filtered-enumerated-red.dat",
                    "ring"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}