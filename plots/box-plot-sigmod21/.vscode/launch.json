    "version": "0.2.0",
    "configurations": [
        {
            "name": "box-plot-gen",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/generate_output",
            "args": ["wikidata_enum_20220714_sigmod21.txt", "query_types.txt"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}